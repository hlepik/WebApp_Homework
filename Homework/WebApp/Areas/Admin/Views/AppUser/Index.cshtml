@model IEnumerable<Domain.App.Identity.AppUser>

@{
    ViewData["Title"] = "Index";
}


<p>
    <a asp-action="Create">@Resources.Views.Crud.CRUD.Create</a>
</p>
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Firstname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lastname)
            </th>
            <th>
              @Resources.Views.AppUser.AppUser.Username
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>

            <th>
               @Resources.Views.AppUser.AppUser.PhoneNumber
            </th>

            <th>
               @Resources.Views.AppUser.AppUser.LockoutEnd
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Firstname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LockoutEnd)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">@Resources.Views.Crud.CRUD.Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">@Resources.Views.Crud.CRUD.Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">@Resources.Views.Crud.CRUD.Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
