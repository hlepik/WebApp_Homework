@model BLL.App.DTO.County

@{
    ViewData["Title"] = "Details";
}

<h1>@Resources.Views.Crud.CRUD.Details</h1>

<div>
    <h4>@Resources.BLL.App.DTO.Counties.County</h4>
    <hr />
      <partial name="_Details" model="Model"/>
</div>
<div>
     @if (User.IsInRole("Admin"))
        {
            <a asp-action="Edit" asp-route-id="@Model.Id">@Resources.Views.Crud.CRUD.Edit</a> <text>|</text>
            <a asp-action="Index">@Resources.Views.Crud.CRUD.Index</a>
        }
        else
        {
            <a asp-action="Index">@Resources.Views.Crud.CRUD.Index</a>
        }
</div>
