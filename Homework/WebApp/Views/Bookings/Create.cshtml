@model WebApp.ViewModels.Booking.BookingCreateEditViewModels

@{
    ViewData["Title"] = "Create";
}

<h4>@Resources.BLL.App.DTO.Bookings.Booking</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="All" class="text-danger"></div>

              <partial name="_CreateEdit" model="Model"/>
            <div class="form-group">
                <input type="submit" value=@Resources.BLL.App.DTO.Bookings.Reserve class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Resources.Views.Crud.CRUD.Index</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
