@model BLL.App.DTO.Product

@{
    ViewData["Title"] = "Details";
}

<dt class = "col-sm-2">
    @Html.DisplayNameFor(model => model.PictureUrls)
</dt>
 @foreach (var picture in Model.PictureUrls!)
   {

       <img src="@Html.Raw(picture)" alt="image" width="250" height="200" class="center">
   }
<div>
    <a asp-action="Index">@Resources.Views.Crud.CRUD.Index</a>
</div>
