@model BLL.App.DTO.Picture

<dl class="row">
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Url)
    </dt>
    <dd class = "col-sm-10">
        <img src="@Html.Raw(Model.Url)" alt="image" style="width: 10rem; height: 8rem">
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Product)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.ProductName)
    </dd >
</dl>