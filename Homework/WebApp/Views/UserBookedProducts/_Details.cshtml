@model BLL.App.DTO.UserBookedProducts
 <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
     <dd class = "col-sm-10">
         @Html.DisplayFor(model => model.Description)
     </dd>
      <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Color)
     </dt>
     <dd class = "col-sm-10">
         @Html.DisplayFor(model => model.Color)
     </dd>
     <dt class = "col-sm-2">
     @Html.DisplayNameFor(model => model.TimeBooked)
     </dt>

     <dd class = "col-sm-10">
         @Html.DisplayFor(model => model.TimeBooked)
     </dd>
     <dt class = "col-sm-2">
           @Html.DisplayNameFor(model => model.Until)
     </dt>

     <dd class = "col-sm-10">
         @Html.DisplayFor(model => model.Until)
     </dd>

    <dt class = "col-sm-2">
   @Html.DisplayFor(model => model.LocationDescription)
    </dt>
    <dd class = "col-sm-10">
    @{
       var location = @Model.County;
   }
     @if (@Model.City != null)
    {
        location += ", " + @Model.City;
    }
    @if (@Model.LocationDescription != null)
    {
        location += ", " + @Model.LocationDescription;
    }
        @location
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.HasTransport)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.HasTransport)
    </dd>

</dl>

